{
  "name": "user-engine",
  "version": "1.0.0",
  "description": "A microservice with nest framework, typeorm, apache kafka and kubernetes.",
  "license": "MIT",
  "author": "Newerton Vargas de Araujo",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./test/unit/jest.config.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --config ./test/jest.config.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/e2e/jest.config.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/axios": "1.0.1",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/mapped-types": "1.2.0",
    "@nestjs/microservices": "9.2.1",
    "@nestjs/platform-express": "9.2.1",
    "env-var": "^7.3.0",
    "joi": "17.7.0",
    "jsonwebtoken": "9.0.0",
    "kafkajs": "2.2.3",
    "keycloak-connect": "21.0.1",
    "lightship": "7.1.1",
    "nest-keycloak-connect": "1.9.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "4.1.2",
    "rxjs": "7.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@nestjs/cli": "9.4.1",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.2.1",
    "@swc/core": "1.3.30",
    "@swc/jest": "0.2.24",
    "@types/express": "4.17.16",
    "@types/jest": "29.4.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/node": "18.11.18",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^8.0.3",
    "jest": "29.4.1",
    "prettier": "2.8.3",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.5.5"
  }
}
